{
  "compilerOptions": {
    "outDir": "./build/",
    "module": "commonjs",
    "target": "ES6",
    "sourceMap": false,
    "inlineSourceMap": true,
    "forceConsistentCasingInFileNames": true,
    "noImplicitReturns": true,
    "noImplicitThis": true,
    "noImplicitAny": true,
    "experimentalDecorators": true,
    "strictNullChecks": false,
    "suppressImplicitAnyIndexErrors": true,
    "noUnusedLocals": false,
    "strict": false,
  }
}